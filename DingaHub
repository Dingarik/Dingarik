local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({
    Name = "Dinga`s Hub", -- name
    HidePremium = false, -- premium user
    SaveConfig = true, -- save config like to save the toggles and things
    ConfigFolder = "OrionTest"
})


MainTab = Window:MakeTab({ -- like a window or tabs you know
    Name = "Universal", -- examople
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- that is a code like MainTab is 1 you can make more tab just make sure yo chnage the name like the MainTab 
MainTab:AddButton({
	Name = "Button!",
	Callback = function()
      		print("button pressed")
  	end    
})

MainTab:AddButton({ -- MainTab like linking to that Windoes tab if you makre more tab you can do the same
    Name = "Button!", -- name of the button
    Callback = function(Value)
          if Value then 
            print("button clicked")
        end
      end    
})

MainTab:AddToggle({
    Name = "This is a toggle!", -- name of the button
    Default = false,
    Callback = function(Value)
        if Value then 
loadstring(game:HttpGet("https://raw.githubusercontent.com/H20CalibreYT/SystemBroken/main/script"))()
        end
    end    
})
